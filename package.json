{
  "name": "Loxe",
  "version": "0.0.1",
  "description": "",
  "author": "ahomu",
  "main": "lib/loxe.js",
  "scripts": {
    "start": "npm run watch & npm run serve",
    "test": "karma start --singleRun",
    "tdd": "karma start",
    "clean": "rm -rf lib/*",
    "lint": "eslint ./src",
    "sync": "browser-sync start --server --logLevel silent --files lib/**",
    "serve": "serve ./",
    "watch": "watchify src/index.js -dv -t [babelify --stage 0] -s Loxe -o lib/loxe.js",
    "build": "npm run concat && npm run minify",
    "concat": "browserify src/index.js -d -t [babelify --stage 0] -s Loxe -o lib/loxe.js",
    "minify": "uglifyjs lib/loxe.js -m -c drop_console=true -o lib/loxe.min.js",
    "patch": "npm version patch -m \"bump v%s !\"",
    "minor": "npm version minor -m \"bump v%s !!\"",
    "major": "npm version major -m \"bump v%s !!!\""
  },
  "dependencies": {
    "baconjs": "^0.7.53",
    "kefir": "^1.3.1",
    "object-assign": "^2.0.0",
    "react": "^0.13.2",
    "smart-mixin": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^5.1.11",
    "babel-eslint": "^3.0.1",
    "babel-plugin-espower": "^0.2.1",
    "babel-runtime": "^5.0.2",
    "babelify": "^6.0.2",
    "browser-sync": "^2.3.1",
    "browserify": "^9.0.3",
    "browserify-istanbul": "^0.2.1",
    "envify": "^3.4.0",
    "eslint": "^0.17.1",
    "espowerify": "^0.10.0",
    "exorcist": "^0.1.6",
    "isparta": "^2.2.0",
    "karma": "^0.12.31",
    "karma-browserify": "^4.0.0",
    "karma-chrome-launcher": "^0.1.7",
    "karma-cli": "0.0.4",
    "karma-coverage": "git://github.com/karma-runner/karma-coverage.git#master",
    "karma-mocha": "^0.1.10",
    "mocha": "^2.2.1",
    "power-assert": "^0.10.2",
    "serve": "^1.4.0",
    "sinon": "^1.14.1",
    "uglifyjs": "^2.4.10",
    "watchify": "^2.4.0"
  }
}
